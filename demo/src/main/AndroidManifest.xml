<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokentm.sdk.uidemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.tokentm.sdk.uidemo.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.tokentm.sdk.uidemo.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tokentm.sdk.uidemo.DidDemoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tokentm.sdk.uidemo.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tokentm.sdk.uidemo.LoginOrRegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tokentm.sdk.uidemo.CertificationDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tokentm.sdk.uidemo.ReleaseCertificateActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tokentm.sdk.uidemo.ChainCertificationActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.tokentm.sdk.uidemo.BackupActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.tokentm.sdk.uidemo.GetBackupActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    </application>

</manifest>