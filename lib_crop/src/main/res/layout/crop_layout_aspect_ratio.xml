<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="horizontal"
    tools:ignore="HardcodedText"
    tools:showIn="@layout/crop_activity_photobox"
    tools:visibility="gone">

    <FrameLayout
        android:id="@+id/crop_aspect_ratio_1_1"
        style="@style/crop_WrapperIconState">

        <com.tokentm.sdk.crop.view.widget.AspectRatioTextView
            style="@style/crop_TextViewCropAspectRatio"
            app:crop_artv_ratio_x="1"
            app:crop_artv_ratio_y="1"/>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/crop_aspect_ratio_3_4"
        style="@style/crop_WrapperIconState">

        <com.tokentm.sdk.crop.view.widget.AspectRatioTextView
            style="@style/crop_TextViewCropAspectRatio"
            app:crop_artv_ratio_x="4"
            app:crop_artv_ratio_y="3"/>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/crop_aspect_ratio_original"
        style="@style/crop_WrapperIconState">

        <com.tokentm.sdk.crop.view.widget.AspectRatioTextView
            style="@style/crop_TextViewCropAspectRatio"
            android:textAllCaps="true"
            app:crop_artv_ratio_title="@string/crop_label_original"/>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/crop_aspect_ratio_3_2"
        style="@style/crop_WrapperIconState">

        <com.tokentm.sdk.crop.view.widget.AspectRatioTextView
            style="@style/crop_TextViewCropAspectRatio"
            app:crop_artv_ratio_x="3"
            app:crop_artv_ratio_y="2"/>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/crop_aspect_ratio_16_9"
        style="@style/crop_WrapperIconState">

        <com.tokentm.sdk.crop.view.widget.AspectRatioTextView
            style="@style/crop_TextViewCropAspectRatio"
            app:crop_artv_ratio_x="16"
            app:crop_artv_ratio_y="9"/>

    </FrameLayout>


</LinearLayout>